<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - DesignPattern</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">DesignPattern</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>
    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>

    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2020 - DesignPattern - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>

    <script src="~/ts/Singleton/StateManager.js"></script>
    <script src="~/ts/Singleton/SingletonMain.js"></script>

    <script src="~/ts/VisitorPattern/Visitor.js"></script>
    <script src="~/ts/VisitorPattern/IEmployee.js"></script>
    <script src="~/ts/VisitorPattern/Employee.js"></script>
    <script src="~/ts/VisitorPattern/Manager.js"></script>
    <script src="~/ts/VisitorPattern/Reporter.js"></script>
    <script src="~/ts/VisitorPattern/Worker.js"></script>
    <script src="~/ts/VisitorPattern//VisitorMain.js"></script>

    <script src="~/ts/CommandPattern/Command.js"></script>
    <script src="~/ts/CommandPattern/SaveCommand.js"></script>
    <script src="~/ts/CommandPattern/UpdateCommand.js"></script>
    <script src="~/ts/CommandPattern/Invoker.js"></script>
    <script src="~/ts/CommandPattern/Receiver.js"></script>
    <script src="~/ts/CommandPattern/CommandMain.js"></script>

    <script src="~/ts/ObserverPattern/Observer.js"></script>
    <script src="~/ts/ObserverPattern/Subject.js"></script>
    <script src="~/ts/ObserverPattern/ConcreteSubject.js"></script>
    <script src="~/ts/ObserverPattern/ObserverMain.js"></script>

    <script src="~/ts/PubSub/PubSub.js"></script>
    <script src="~/ts/PubSub/PubSubMain.js"></script>

    @RenderSection("Scripts", required: false)
</body>
</html>
